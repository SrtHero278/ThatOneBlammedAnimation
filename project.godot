config_version=5

[application]

config/name="That One Blammed Animation"
run/main_scene="res://scenes/pause/PauseMenu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/max_fps=120
config/icon="res://picoIcon.png"

[autoload]

Overlay="*res://autoload/Overlay.tscn"
Conductor="*res://autoload/Conductor.gd"
Config="*res://autoload/Config.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/parallax_node/plugin.cfg")

